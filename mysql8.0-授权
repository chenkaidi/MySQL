create user 'jct_readonly'@'localhost' identified by 'amN0X3JlYWRvbmx5';
GRANT select ON dtct.* TO 'jct_readonly'@'localhost';

create user 'jct_readonly'@'%' identified by 'amN0X3JlYWRvbmx5';
GRANT select ON dtct.* TO 'jct_readonly'@'%';

ALTER USER 'jct_readonly'@'%' IDENTIFIED WITH mysql_native_password BY 'amN0X3JlYWRvbmx5';
ALTER USER 'jct_readonly'@'localhost' IDENTIFIED WITH mysql_native_password BY 'amN0X3JlYWRvbmx5';

FLUSH PRIVILEGES;
